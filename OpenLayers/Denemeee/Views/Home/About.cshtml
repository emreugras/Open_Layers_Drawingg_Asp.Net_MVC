                setTimeout(function () { box.style.opacity = 0 }, 150000);
            });
        </script>*@
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"></script>
    <script type="module" src="~/Scripts/Page/Main.js"></script>
</body>

</html>